  "scripts": {
    "dev": "dotenv -e '../.env' -e '../.env.defaults' -- cross-var webpack-dev-server --config webpack/webpack.dev.config.js --port %RAVEN_UI_DEV_PORT% --progress --color",
    "prebuild": "rimraf dist",
    "build": "webpack --config webpack/webpack.prod.config.js",
    "profile": "webpack --config webpack/webpack.profile.config.js",
    "server": "dotenv -e '../.env' -e '../.env.defaults' -- cross-var http-server -p %RAVEN_UI_PORT%  --proxy %RAVEN_UI_HOST%:%RAVEN_UI_PORT%? ./dist -c 31536000 --gzip --log-ip",
    "start": "npm run build && npm run server"
